{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "posthog:default",
    "posthog:allow-capture",
    "posthog:allow-identify",
    "posthog:allow-get-config",
    "posthog:allow-get-distinct-id",
    "posthog:allow-reset",
    "posthog:allow-alias"
  ]
}
