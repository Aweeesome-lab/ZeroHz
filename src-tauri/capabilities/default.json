{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "log:default",
    "log:allow-log",
    "posthog:default",
    "posthog:allow-capture",
    "posthog:allow-identify",
    "posthog:allow-get-config",
    "posthog:allow-get-distinct-id",
    "posthog:allow-reset",
    "posthog:allow-alias",
    "store:default",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "opener:default",
    "opener:allow-open-url",
    "autostart:default"
  ]
}
